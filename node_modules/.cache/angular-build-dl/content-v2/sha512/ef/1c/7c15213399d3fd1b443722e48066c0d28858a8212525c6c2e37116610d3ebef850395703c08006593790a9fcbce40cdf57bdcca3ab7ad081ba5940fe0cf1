        this.isPlaceOrder = false;
        this.options = {
            key: 'rzp_test_p3yS9vCKJBJ4FJ',
            name: 'The Swag Coder',
            description: 'Shopping',
            amount: 100,
            prefill: {
                name: 'The Swag Coder',
                email: 'jithendhra567@gmail.com',
            },
            notes: {},
            theme: {
                color: '#3880FF'
            },
